{
  "id": null,
  "title": "Adaptive vs Round-Robin Scheduler Comparison",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Worker CPU Usage (Adaptive vs RR)",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 8 },
      "targets": [
        {
          "expr": "worker_cpu_usage",
          "legendFormat": "{{scheduler}} / {{worker}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Task Latency p95 (Adaptive vs RR)",
      "gridPos": { "x": 0, "y": 8, "w": 24, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(task_latency_seconds_bucket[1m]))",
          "legendFormat": "{{scheduler}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Task Throughput (tasks/sec)",
      "gridPos": { "x": 0, "y": 16, "w": 24, "h": 8 },
      "targets": [
        {
          "expr": "rate(task_latency_seconds_count[1m])",
          "legendFormat": "{{scheduler}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    }
  ]
}
